{
  "compilerOptions": {
    "target": "es2015",
    "module": "CommonJS",
    "declaration": true,
    "lib": ["es2015", "DOM"],
    "outDir": "./dist",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitOverride": true,
    "moduleResolution": "node"
  },
  "exclude": ["dist"]
}
